   "source": [
    "def rating_feature(feature_row):\n",
    "    ind = pd.Index(df['title'])\n",
    "    arr = ind.str.contains(feature_row['Feature'], regex=False)\n",
    "    l = []\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i]:\n",
    "            l.append(df.iloc[i]['rating_numeric'])\n",
    "    return mean(l)\n",
    "    \n",
    "            \n",
    "\n",
    "\n"
   ]
