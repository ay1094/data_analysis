   "source": [
    "def rating_numeric(row):\n",
    "    ratnum = row['rating'].split(' ')[0]\n",
    "    return ratnum\n",
    "\n",
    "df['rating_numeric'] = df.apply(lambda x: rating_numeric(x), axis=1)\n",
    "df['rating_numeric'] = df.rating_numeric.astype(float)"
   ]
